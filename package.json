{
  "name": "template-node-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/src/shared/infra/http/server.js",
    "typeorm": "MIGRATION=TRUE node ./node_modules/typeorm/cli.js -d ./dist/src/shared/infra/typeorm/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules ./src/shared/infra/http/server.ts",
    "build": "yarn babel src --extensions \".js,.ts\" --out-dir dist/src --copy-files --no-copy-ignored",
    "test": "jest",
    "seed:admin": "ts-node-dev -r tsconfig-paths/register ./src/shared/infra/typeorm/seed/admin.ts",
    "seed:admin.prod":"node ./dist/src/shared/infra/typeorm/seed/admin.js",
    "docs:build": "ts-node-dev -r tsconfig-paths/register ./src/docs/build.ts",
    "perf": "ts-node-dev -r tsconfig-paths/register ./src/shared/infra/typeorm/tests/users/createUser.performance.ts"
  },
  "heroku-run-build-script": true,
  "repository": "https://github.com/kortkamp/template-node-api.git",
  "author": "kortkamp <marcelusmedius@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^7.6.2",
    "aws-sdk": "^2.1111.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.1",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "mime": "^3.0.0",
    "mongodb": "^4.4.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.3",
    "rate-limiter-flexible": "^2.3.6",
    "redis": "3.1.2",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^4.0.0",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.3.6",
    "typeorm-dynamic-filters": "^0.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mime": "^2.0.3",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "@types/redis": "^2.8.32",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
